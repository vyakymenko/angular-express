{
  "name": "angular-express",
  "version": "10.1.5",
  "description": "Modular starter project for Full Stack Angular 2 (and beyond)/Express with Angular CLI, PM2",
  "repository": {
    "type": "git",
    "url": "https://github.com/vyakymenko/angular-express"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "pm2": "pm2",
    "nodemon": "nodemon",
    "lint.ui": "ng lint",
    "watch.server": "tsc -p server -watch",
    "e2e": "npm run build.prod && npm run e2e.run",
    "e2e.live": "npm run build.prod && node dist/server/app.js & cypress open",
    "e2e.run": "node dist/server/app.js & cypress run",
    "test.ui": "ng test --watch=false",
    "test.ui.watch": "ng test",
    "test.server": "npm run build.server && mocha dist/server/**/*.spec.js --exit",
    "test.all": "npm run test.server && ng test --watch=false && npm run build.prod && npm run e2e ",
    "build.server": "tsc -p server",
    "build.dev": "npm run build.server && ng build",
    "build.prod": "ng build --prod && npm run build.server",
    "run.prod": "pm2 start dist/server/app.js",
    "run.server": "npm run watch.server & nodemon dist/server/app.js",
    "start": "npm run watch.server & nodemon dist/server/app.js & ng serve --host 0.0.0.0",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "optionalDependencies": {
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.0",
    "@cypress/webpack-preprocessor": "^5.0.0",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^7.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cypress": "^4.0.0",
    "mocha": "^7.0.1"
  },
  "dependencies": {
    "@angular/animations": "10.2.2",
    "@angular/common": "10.2.2",
    "@angular/compiler": "10.2.2",
    "@angular/core": "10.2.2",
    "@angular/forms": "10.2.2",
    "@angular/platform-browser": "10.2.2",
    "@angular/platform-browser-dynamic": "10.2.2",
    "@angular/router": "10.2.2",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "1.7.0",
    "@types/express": "^4.17.11",
    "@types/redis": "^2.8.26",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "knex": "^0.21.13",
    "morgan": "^1.9.1",
    "mysql2": "^2.2.5",
    "objection": "^1.6.9",
    "pg": "^8.5.1",
    "pm2": "^4.5.1",
    "redis": "^3.0.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2",
    "snyk": "^1.605.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.6",
    "@angular/cli": "10.1.6",
    "@angular/compiler-cli": "10.1.5",
    "@angular/language-service": "10.1.5",
    "@types/morgan": "^1.7.35",
    "@types/node": "^13.11.1",
    "codelyzer": "^5.1.2",
    "nodemon": "^2.0.0",
    "ts-node": "~8.9.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  },
  "snyk": true
}
